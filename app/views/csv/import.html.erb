<h1>Csv#import</h1>

<%= form_for :upload, :html => { :multipart => true } do |f| %>

  <div class="file_upload">

        <span class="button">Выбрать</span>
        <mark>Файл не выбран</mark>
    <%= f.file_field :avatar %>   
    <%= f.hidden_field :avatar_cache %> 
  </div>

  <%= f.submit "Upload", :disable_with => 'Uploading...' %>

<% end %>



